{
  "name": "rodin-mobile",
  "main": "index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start -c --go",
    "reset-project": "node utils/scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:ios": "eas build -p ios --profile development",
    "build:android": "eas build --platform android --profile preview --local",
    "open:xcode": "open ios/rodinmobile.xcworkspace",
    "web": "expo start --web",
    "test": "jest --updateSnapshot --coverage",
    "lint": "expo lint",
    "models-reinstall": "npm uninstall @rodinwm/rodin-models && npm install @rodinwm/rodin-models",
    "package:login": "npm login --registry=https://npm.pkg.github.com"
  },
  "jest": {
    "preset": "jest-expo",
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageDirectory": "./__tests__/coverage"
  },
  "jestSonar": {
    "reportPath": "./__tests__/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@faker-js/faker": "^9.5.1",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.1.7",
    "@rodinwm/rodin-models": "^1.0.0",
    "@testing-library/react-native": "^13.2.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "expo": "53.0.20",
    "expo-asset": "~11.1.5",
    "expo-audio": "~0.4.7",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.6",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.2",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "gifted-charts-core": "^0.1.63",
    "lucide-react-native": "^0.515.0 ",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-bouncy-checkbox": "^4.1.2",
    "react-native-device-activity": "^0.4.31",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "6.7.1",
    "react-native-permissions": "^5.4.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-table-component": "^1.2.2",
    "react-native-timer-picker": "^2.1.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-native-wheely": "^0.6.0",
    "tailwindcss": "^3.4.17",
    "toastify-react-native": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-native-table-component": "^1.2.8",
    "eas-cli": "^16.10.1",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.7",
    "jest-sonar-reporter": "^2.0.0",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  }
}
